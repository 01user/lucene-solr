{
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "A name for the library"
    },
    "blobs": {
      "type": "array",
      "items": {
       "#include" :"cluster.package.Commands.blob.properties"
      }
    },
    "blob": {
      "#include" :"cluster.package.Commands.blob.properties"
    },
    "repository": {
      "type": "string",
      "description": "the repository name"
    },
    "version": {
      "type": "string",
      "description": "version of he package"
    },
    "metadata": {
      "type": "string",
      "description": "SHA256 of the metadata json, stored in the FSBlobStore"
    }
  },
  "required": [
    "name",
    "version"
  ],
  "oneOf" : ["blob","blobs"]
}
